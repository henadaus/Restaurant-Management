<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="561.0" prefWidth="799.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="staff.Manager.ManagerWindowController">
   <children>
      <BorderPane prefHeight="561.0" prefWidth="799.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <top>
            <HBox prefHeight="100.0" prefWidth="200.0" spacing="150.0" BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" onAction="#onClickStart" prefHeight="100.0" prefWidth="130.0" text="START">
                     <effect>
                        <Lighting>
                           <bumpInput>
                              <Shadow />
                           </bumpInput>
                           <light>
                              <Light.Distant />
                           </light>
                        </Lighting>
                     </effect>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#onClickgetPendingOrder" prefHeight="100.0" prefWidth="130.0" text="REFRESH">
                     <effect>
                        <Lighting>
                           <bumpInput>
                              <Shadow />
                           </bumpInput>
                           <light>
                              <Light.Distant />
                           </light>
                        </Lighting>
                     </effect>
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#onClickForward" prefHeight="100.0" prefWidth="130.0" text="FORWARD">
                     <effect>
                        <Lighting>
                           <bumpInput>
                              <Shadow />
                           </bumpInput>
                           <light>
                              <Light.Distant />
                           </light>
                        </Lighting>
                     </effect>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#onClickCancel" prefHeight="100.0" prefWidth="130.0" text="CANCEL">
                     <effect>
                        <Lighting>
                           <bumpInput>
                              <Shadow />
                           </bumpInput>
                           <light>
                              <Light.Distant />
                           </light>
                        </Lighting>
                     </effect>
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </top>
         <center>
            <TableView fx:id="processingOrderTable" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="ProcessingOrderColumn" prefWidth="75.0" text="Order ID" />
                <TableColumn fx:id="ProcessingCustomerColumn" prefWidth="75.0" text="Customer ID" />
                  <TableColumn fx:id="ProcessingTableColumn" prefWidth="74.0" text="Table ID" />
                  <TableColumn fx:id="remTimeColumn" prefWidth="112.0" text="Remaining Time" />
                  <TableColumn fx:id="totalTimeColumn" prefWidth="75.0" text="Total Time" />
                  <TableColumn fx:id="progressColumn" prefWidth="88.0" text="Progress" />
                  <TableColumn fx:id="waiterIDColumn" prefWidth="100.0" text="Waiter ID" />
              </columns>
            </TableView>
         </center>
         <right>
            <ListView prefHeight="250.0" prefWidth="211.0" BorderPane.alignment="CENTER" />
         </right>
      </BorderPane>
   </children>
</AnchorPane>
